# Configurações do Banco de Dados PostgreSQL
DATABASE_URL=postgresql://username:password@localhost:5432/cofre_senhas_db
DB_HOST=localhost
DB_USER=username
DB_PASSWORD=password
DB_NAME=cofre_senhas_db
DB_PORT=5432

# Configurações da Aplicação
NODE_ENV=production
PORT=3000
SESSION_SECRET=seu-secret-muito-seguro-aqui-2024

# Configurações de Segurança
BCRYPT_ROUNDS=12

# Configurações do Render (para produção)
# Substitua pelos valores reais fornecidos pelo Render
RENDER_DATABASE_URL=postgresql://user:pass@host:port/database
RENDER_EXTERNAL_URL=https://seu-app.onrender.com

# Configurações de Email (opcional)
